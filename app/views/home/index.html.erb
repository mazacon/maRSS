<h1>Welcome to maRSS!</h1>
<%= form_tag '/login' do %>
   <%= label_tag :username%>
   <%= text_field_tag :username %>
   <%= label_tag :password%>
   <%= password_field_tag :password%>
   <%= submit_tag "Login"%>
<%end%>

<% if logged_in? %>
    <h1>you shouldn't see this</h1>
<%end%>

<%= button_to "Sign Up", '/signup', method: :get%>
